{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import",
        "babel"
    ],
    "rules": {
        "react/prefer-stateless-function": [0, { "ignorePureComponents": 0 }],
        "react/prop-types": 0,
        "linebreak-style": 0,
        "no-console": 0,
        "no-underscore-dangle": 0
    },
    "env": {
        "browser": true
    }
}